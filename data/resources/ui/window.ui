<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="MsaiWindow" parent="AdwApplicationWindow">
    <property name="content">
      <object class="GtkStack" id="main_stack">
        <property name="transition-type">over-left-right</property>
        <child>
          <object class="MsaiMainPage" id="main_page"/>
        </child>
        <child>
          <object class="MsaiSongPage" id="song_page"/>
        </child>
      </object>
    </property>
  </template>
</interface>
